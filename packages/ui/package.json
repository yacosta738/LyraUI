{
  "name": "@lyra/ui",
  "description": "UI components for Lyra",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/lyra-vue-ui.umd.js",
  "module": "./dist/lyra-vue-ui.mjs",
  "types": "./dist/src/index.d.ts",
  "styles": "./dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/lyra-vue-ui.umd.js",
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      }
    }
  },
  "keywords": [
    "vue",
    "common ui"
  ],
  "scripts": {
    "build:types": "vue-tsc --noEmit && tsc-alias -p tsconfig.json",
    "build:vite": "vite build",
    "check": "vue-tsc --noEmit",
    "build": "run-p build:vite build:types",
    "clean": "rm -rf dist",
    "test:unit": "vitest --coverage --run",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "typescript": "^5.4.2",
    "vue": "^3.4.21"
  },
  "dependencies": {
    "@tiptap/core": "^2.2.4",
    "@tiptap/extension-blockquote": "^2.2.4",
    "@tiptap/extension-bold": "^2.2.4",
    "@tiptap/extension-bullet-list": "^2.2.4",
    "@tiptap/extension-color": "^2.2.4",
    "@tiptap/extension-document": "^2.2.4",
    "@tiptap/extension-dropcursor": "^2.2.4",
    "@tiptap/extension-gapcursor": "^2.2.4",
    "@tiptap/extension-hard-break": "^2.2.4",
    "@tiptap/extension-heading": "^2.2.4",
    "@tiptap/extension-highlight": "^2.2.4",
    "@tiptap/extension-history": "^2.2.4",
    "@tiptap/extension-horizontal-rule": "^2.2.4",
    "@tiptap/extension-image": "^2.2.4",
    "@tiptap/extension-italic": "^2.2.4",
    "@tiptap/extension-link": "^2.2.4",
    "@tiptap/extension-list-item": "^2.2.4",
    "@tiptap/extension-ordered-list": "^2.2.4",
    "@tiptap/extension-paragraph": "^2.2.4",
    "@tiptap/extension-strike": "^2.2.4",
    "@tiptap/extension-text": "^2.2.4",
    "@tiptap/extension-text-align": "^2.2.4",
    "@tiptap/extension-text-style": "^2.2.4",
    "@tiptap/extension-underline": "^2.2.4",
    "@tiptap/pm": "^2.2.4",
    "@tiptap/vue-3": "^2.2.4",
    "@unocss/reset": "^0.58.5",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "date-fns": "^3.4.0",
    "echarts": "^5.5.0",
    "ofetch": "^1.3.3",
    "pinia": "^2.1.7",
    "shiki": "^1.1.7",
    "vue": "^3.4.21",
    "vue-component-type-helpers": "^2.0.6",
    "vue-echarts": "^6.6.9",
    "vue-imask": "^7.5.0",
    "vue-localer": "^1.4.1",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@unocss/postcss": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@iconify/json": "^2.2.191",
    "@lyra/config": "workspace:^",
    "@lyra/tsconfig": "0.0.1",
    "@lyra/utilities": "0.0.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "tsc-alias": "1.8.8",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "vite-plugin-dts": "^3.7.3",
    "vue-tsc": "^2.0.6"
  },
  "engines": {
    "node": ">=18.14.1",
    "pnpm": ">=8.6.12"
  },
  "packageManager": "pnpm@8.14.1"
}
